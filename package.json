{
  "name": "ts-is-defined",
  "version": "3.0.3",
  "description": "Typescript utilities",
  "sideEffects": false,
  "types": "./dist/dts/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/mjs/index.js",
  "react-native": "./dist/mjs/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/mjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "scripts": {
    "preversion": "npm run lint && npm run build",
    "postversion": "git push && git push --tags",
    "build:ts": "tsc -p configs/tsconfig.cjs.json && tsc -p configs/tsconfig.mjs.json && tsc -p configs/tsconfig.dts.json",
    "build": "rm -rf dist && npm run build:ts && echo '{ \"type\": \"commonjs\" }' >> dist/cjs/package.json",
    "lint": "tslint -p . && tslint test/*",
    "watch": "tsc -p . -w"
  },
  "keywords": [
    "typescript",
    "assert"
  ],
  "files": [
    "dist",
    "src"
  ],
  "author": "iyegoroff <iegoroff@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iyegoroff/ts-is-defined/issues"
  },
  "homepage": "https://github.com/iyegoroff/ts-is-defined#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyegoroff/ts-is-defined.git"
  },
  "devDependencies": {
    "tslint": "^6.1.3",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^4.6.3"
  },
  "prettier": {
    "trailingComma": "none",
    "singleQuote": true,
    "semi": false
  },
  "dependencies": {
    "ts-tiny-invariant": "^2.0.4"
  }
}
